#ifndef JOB_H_
#define JOB_H_

#include <ilcplex/ilocplex.h>
#include <ilconcert/ilocsvreader.h>
#include <ilconcert/iloexpression.h>

namespace std {

class Job {
public:
	Job(IloNum s, IloNum p, IloNum d);
	IloNum s, p, d;
	bool operator<(const Job &a, const Job &b) {return a.d < b.d;}
};

} /* namespace std */
#endif /* JOB_H_ */
