#ifndef ROOTNODE_H_
#define ROOTNODE_H_

namespace std {

class RootNode {
public:
	RootNode(vector<Job> *all_jobs);
	virtual ~RootNode();

	int run();

private:
	vector<Job> *all_jobs;
};

} /* namespace std */
#endif /* ROOTNODE_H_ */
