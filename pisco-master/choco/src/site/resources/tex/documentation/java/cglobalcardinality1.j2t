        int n = 5;
        Model m = new CPModel();
        Solver s = new CPSolver();
        IntegerVariable[] vars = new IntegerVariable[n];
        for (int i = 0; i < n; i++) {
           vars[i] = makeIntVar("var " + i, 1, n);
        }
        int[] LB2 = {0, 1, 1, 0, 3};
        int[] UB2 = {0, 1, 1, 0, 3};
        m.addConstraint(Options.C_GCC_BC, globalCardinality(vars, LB2, UB2, 1));
        s.read(m);
        s.solve();
