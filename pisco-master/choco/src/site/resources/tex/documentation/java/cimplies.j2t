        Model m = new CPModel();
        Solver s = new CPSolver();
        IntegerVariable x = makeIntVar("x", 1, 2);
        IntegerVariable y = makeIntVar("y", 1, 2);
        IntegerVariable z = makeIntVar("z", 1, 2);
        m.addVariables(Options.V_BOUND,x ,y, z);
        Constraint e1 = implies(leq(x, y), leq(x, z));
        m.addConstraint(e1);
        s.read(m);
        s.solveAll();
